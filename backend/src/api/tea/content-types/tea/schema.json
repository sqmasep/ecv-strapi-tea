{
  "kind": "collectionType",
  "collectionName": "teas",
  "info": {
    "singularName": "tea",
    "pluralName": "teas",
    "displayName": "Tea"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "infusionTime": {
      "type": "integer",
      "default": 0
    },
    "temperature": {
      "type": "integer"
    },
    "dose": {
      "type": "string"
    },
    "tea_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tea-type.tea-type",
      "inversedBy": "teas"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "dynamicZone": {
      "type": "dynamiczone",
      "components": [
        "dynamic-zone.illustration-section",
        "dynamic-zone.rich-text",
        "dynamic-zone.gallery",
        "dynamic-zone.cta-banner",
        "dynamic-zone.key-facts",
        "dynamic-zone.quote"
      ]
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "favorites"
    }
  }
}
